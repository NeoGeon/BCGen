<models>
	<model>
		<name>a</name>
                <attrVect>  <!--local attrVect-->
			<name></name><!--optional-->
			<lsize></lsize>
			<nx>100</nx>
			<ny>100</ny>
     			<field>lon:lat</field>
		</attrVect>
		<init> <!--if user not define the optional tag then generate default subroutine-->
			<name>a_init_mct</name><!--optional-->
                        <arg>gsMap_aa</arg><!--optional-->
			<arg>a2x_aa</arg>    <!-- optional-->
			<arg>x2a_aa</arg>   <!--optional-->
		</init>
		<run>
			<name>a_run_mct</name>
			<arg>gsMap_aa</arg>
                        <arg>a2x_aa</arg>
                        <arg>x2a_aa</arg>
		</run>
		<final>
			<name>a_final_mct</name>	
			<arg>gsMap_aa</arg>
 			<arg>a2x_aa</arg>
			<arg>x2a_aa</arg>
		</final>
	</model>
