<models>
	<model>
		<name>a</name>
		<interval>3</interval>
                <attrVect>  <!--local attrVect-->
			<name></name><!--optional-->
			<gsize>12</gsize>
			<nx>3</nx><!--this can be decided by grid file then-->
			<ny>4</ny><!--same as nx-->
     			<field>vel:hit</field><!--same as ny-->
		</attrVect>
		<grid>
			<field>lat:lon:area:mask:frac</field><!--optional-->
			<file></file>
		</grid>
		<init> <!--if user not define the optional tag then generate default subroutine-->
			<name>a_init_mct</name><!--optional-->
                        <arg>gsMap_aa</arg><!--optional-->
			<arg>a2x_aa</arg>    <!-- optional-->
			<arg>x2a_aa</arg>   <!--optional-->
			<arg>grid_a</arg>   <!--optional??-->
		</init>
		<run>
			<name>a_run_mct</name>
			<arg>gsMap_aa</arg>
                        <arg>a2x_aa</arg>
                        <arg>x2a_aa</arg>
			<arg>grid_a</arg>
		</run>
		<final>
			<name>a_final_mct</name>	
			<arg>gsMap_aa</arg>
 			<arg>a2x_aa</arg>
			<arg>x2a_aa</arg>
		</final>
	</model>
